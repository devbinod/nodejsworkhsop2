<h1>Student List</h1>

<a href="addStudent" class="heading">Add Student</a>


<table width='100%' border="2">
    <tr>
        <td> First Name
        </td>

        <td>Last Name</td>
        <td>Age</td>
        <td>Grade</td>
        <td>Action</td>
    </tr>

    {{#each studentList}}
    <tr>
        <td> {{this.firstName}}
        </td>

        <td>{{this.lastName}}</td>
        <td>{{this.age}}</td>
        <td>{{this.grade}}</td>
        <td><a href="/delete?id={{this._id}}">Delete</a>

            <a href="/update?id={{this._id}}">Update</a>
        </td>
    </tr>

    {{/each}}
</table>